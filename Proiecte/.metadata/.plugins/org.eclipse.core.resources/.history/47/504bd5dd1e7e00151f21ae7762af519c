
public class Program {

	public static void main(String[] args) {
		
		String mesaj = "";
		
		String tara = "RO";
		if(tara == "RO"){
			mesaj = "Salut";
		}else{
			mesaj = "Hello";
		}
		
		System.out.println(mesaj);
		System.out.println(mesaj);
		System.out.println(mesaj);
		System.out.println(mesaj);
		System.out.println(mesaj);
		
		
	}
}
